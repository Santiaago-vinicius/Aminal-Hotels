generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Tutor {
  id       String   @id @default(uuid())
  name     String
  email    String   @unique
  phone    String
  password String
  animals  Animal[]
}

model Animal {
  id      String @id @default(uuid())
  name    String
  species String
  breed   String
  age     Int
  tutorId String
  tutor   Tutor  @relation(fields: [tutorId], references: [id])
}